        <label for="pwd">Password</label>
        <br>
        <input type="password" placeholder="Enter Your Password" name="password">
        <br>
        <input type="submit" id="Login">
        <br>
        <p>new user?</p>
        <a href="register">Click here for registration</a>
    </form>
</body>
</html>